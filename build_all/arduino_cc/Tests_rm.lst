[huzzah_remote_monitoring]
Target=remote_monitoring.ino
RelativePath=\iot-hub-c-huzzah-getstartedkit\remote_monitoring
RelativeWorkingDir=\arduino_cc_Work\Results\huzzah_remote_monitoring
CloneURL=https://github.com/Azure-Samples/iot-hub-c-huzzah-getstartedkit
MaxAllowedDurationSeconds=100
LogLines=300
MinimumHeap=7000
Categories=IoTHub
Hardware=Adafruit_Huzzah
SerialPort=COM4
CPUParameters=-fqbn=esp8266:esp8266:huzzah:CpuFrequency=80,UploadSpeed=115200,FlashSize=4M3M -ide-version=10609
Build=Enable
Execution=Enable

